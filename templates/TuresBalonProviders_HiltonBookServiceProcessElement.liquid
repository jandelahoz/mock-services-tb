<soapenv:Envelope xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:hil="http://xmlns.oracle.com/HiltonRoomService">
   <soapenv:Header/>
   <soapenv:Body>
      <hil:HiltonRoomServiceProcessResponse>
         <hil:result>
            <hil:Rooms>
               <hil:Number>{{ data.number1 }}</hil:Number>
               <hil:Hotel>
                  <hil:Name>{{ data.name }}</hil:Name>
                  <hil:Address>{{ data.address }}</hil:Address>
                  <hil:City>{{ data.city }}</hil:City>
                  <hil:Country>{{ data.country }}</hil:Country>
               </hil:Hotel>
               <hil:Price>{{ data.price1 }}</hil:Price>
               <hil:Type>{{ data.type }}</hil:Type>
            </hil:Rooms>
            <hil:Rooms>
               <hil:Number>{{ data.number2 }}</hil:Number>
               <hil:Hotel>
                  <hil:Name>{{ data.name }}</hil:Name>
                  <hil:Address>{{ data.address }}</hil:Address>
                  <hil:City>{{ data.city }}</hil:City>
                  <hil:Country>{{ data.country }}</hil:Country>
               </hil:Hotel>
               <hil:Price>{{ data.price2 }}</hil:Price>
               <hil:Type>{{ data.type }}</hil:Type>
            </hil:Rooms>
            <hil:Rooms>
               <hil:Number>{{ data.number3 }}</hil:Number>
               <hil:Hotel>
                  <hil:Name>{{ data.name }}</hil:Name>
                  <hil:Address>{{ data.address }}</hil:Address>
                  <hil:City>{{ data.city }}</hil:City>
                  <hil:Country>{{ data.country }}</hil:Country>
               </hil:Hotel>
               <hil:Price>{{ data.price3 }}</hil:Price>
               <hil:Type>{{ data.type }}</hil:Type>
            </hil:Rooms>
         </hil:result>
      </hil:HiltonRoomServiceProcessResponse>
   </soapenv:Body>
</soapenv:Envelope>